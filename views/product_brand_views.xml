<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    
    <record id="jm_view_product_brand_form" model="ir.ui.view">
        <field name="model">product.brand</field>
        <field name="inherit_id" ref="product_brand.view_product_brand_form" />
        <field name="arch" type="xml">
            <field name="partner_id" position="after">
                <field name="sequence"/>
            </field>
        </field>
    </record>

    <record id="jm_view_product_brand_view" model="ir.ui.view">
        <field name="model">product.brand</field>
        <field name="inherit_id" ref="product_brand.view_product_brand_tree" />
        <field name="arch" type="xml">
            <field name="partner_id" position="after">
                <field name="sequence"/>
            </field>
        </field>
    </record>

</odoo>
